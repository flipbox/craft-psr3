{
  "name": "flipboxdigital/craft-psr3",
  "description": "PSR3 Logger for Craft CMS",
  "version": "1.0.1",
  "keywords": [
    "psr-3",
    "psr3",
    "craft",
    "craft cms",
    "yii2"
  ],
  "license": "MIT",
  "authors": [
    {
      "name": "Flipbox Factory",
      "email": "hello@flipboxfactory.com"
    }
  ],
  "require": {
    "php": ">=7.0",
    "psr/log": "^1.0.1",
    "craftcms/cms": "~3.0.0-beta.19"
  },
  "require-dev": {
    "phpunit/phpunit": "^5.0",
    "squizlabs/php_codesniffer": "^2.0"
  },
  "autoload": {
    "psr-4": {
      "flipbox\\craft\\psr3\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "flipbox\\craft\\psr3\\tests\\": "tests/src/"
    }
  },
  "config": {
    "optimize-autoloader": true
  },
  "extra": {
    "branch-alias": {
      "dev-master": "1.0.x-dev"
    }
  },
  "repositories": [
    {
      "type": "composer",
      "url": "https://packagist.org"
    },
    {
      "type": "composer",
      "url": "https://asset-packagist.org"
    }
  ]
}